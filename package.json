        "test:watch": "vitest",
        "test:cov": "vitest --coverage"
    },
    "keywords": [
        "titan planet",
        "extension"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "devalue": "^5.6.2"
    },
    "peerDependencies": {
        "@titanpl/core": "latest"
    },
    "devDependencies": {
        "@titanpl/core": "latest",
        "@vitest/coverage-v8": "^4.0.17",
        "esbuild": "^0.27.2",
        "titanpl-sdk": "latest",
        "vitest": "^4.0.17"
    }
}